{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "tsconfig/nuxt.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.vue"],
  "exclude": ["node_modules"]
}
